<h1>Listing merchants</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th>Contact name</th>
    <th>Address</th>
    <th>Authenticateable</th>
    <th>Authenticateable type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @merchants.each do |merchant| %>
  <tr>
    <td><%= merchant.name %></td>
    <td><%= merchant.email %></td>
    <td><%= merchant.phone %></td>
    <td><%= merchant.contact_name %></td>
    <td><%= merchant.address %></td>
    <td><%= merchant.authenticateable_id %></td>
    <td><%= merchant.authenticateable_type %></td>
    <td><%= link_to 'Show', merchant %></td>
    <td><%= link_to 'Edit', edit_merchant_path(merchant) %></td>
    <td><%= link_to 'Destroy', merchant, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Merchant', new_merchant_path %>
